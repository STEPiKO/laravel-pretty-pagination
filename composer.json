{
    "name": "vipertecpro/laravel-pretty-pagination",
    "description": "Laravel SEO friendly pagination routing.",
    "keywords": [
        "vipertecpro",
        "laravel-pretty-pagination",
        "SEO",
        "laravel",
        "pagination",
        "routing"
    ],
    "homepage": "https://github.com/vipertecpro/laravel-pretty-pagination",
    "license": "MIT",
    "authors": [
        {
            "name": "Vipul Walia",
            "email": "vipertecpro@gmail.com",
            "homepage": "https://www.vipertecpro.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/contracts": "6.*",
        "illuminate/routing": "6.*",
        "illuminate/translation": "6.*",
        "illuminate/pagination": "6.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.0",
        "roave/security-advisories": "dev-master",
        "orchestra/testbench": "~3.8.0|^4.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Vipertecpro\\PaginateRoute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vipertecpro\\PaginateRoute\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Vipertecpro\\PaginateRoute\\PaginateRouteServiceProvider"
            ],
            "aliases": {
                "PaginateRoute": "Vipertecpro\\PaginateRoute\\PaginateRouteFacade"
            }
        }
    },
    "minimum-stability": "stable"
}
