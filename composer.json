{
    "name": "stepiko/laravel-pretty-pagination",
    "description": "Laravel SEO friendly pagination routing.",
    "keywords": [
        "stepiko",
        "laravel-pretty-pagination",
        "SEO",
        "laravel",
        "pagination",
        "routing"
    ],
    "homepage": "https://github.com/stepiko/laravel-pretty-pagination",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Stepanov",
            "email": "stepiko79@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "5.8.*|^6|^7|^8",
        "illuminate/routing": "5.8.*|^6|^7|^8",
        "illuminate/translation": "5.8.*|^6|^7|^8",
        "illuminate/pagination": "5.8.*|^6|^7|^8",
        "ext-json": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Stepiko\\PaginateRoute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stepiko\\PaginateRoute\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Stepiko\\PaginateRoute\\PaginateRouteServiceProvider"
            ],
            "aliases": {
                "PaginateRoute": "Stepiko\\PaginateRoute\\PaginateRouteFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
